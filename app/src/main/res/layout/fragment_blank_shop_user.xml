<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.User.BlankFragmentShopUser">
    <ViewFlipper
        android:id="@+id/view_flipper_shop_user"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <!--        layout tài khoản -->
        <include
            android:id="@+id/include_shopping_cart_user"
            layout="@layout/shopping_cart_user"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#EFE2E2"/>
        <!--       layout cài đặt tài khoản-->
        <include
            android:id="@+id/include_promotion_user"
            layout="@layout/promotion_user"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#EFE2E2"/>
    </ViewFlipper>
</FrameLayout>